<header>
  <div class="site-title">
    Jabstat
  </div>
  <div class="server-name">
    Contests
  </div>
</header>


<aside class="sidebar">
  <ul class="server-list">
    <li class="logo online">
      <a href="/">
        <img src="images/bot-logo.png" alt="Drawmania Logo">
      </a>
      Jabstat
    </li>
    <li class="break"></li>
    <!-- {{#each servers}}
